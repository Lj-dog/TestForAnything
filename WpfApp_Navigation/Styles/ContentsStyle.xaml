<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:m="clr-namespace:WpfApp_Navigation.Models">
    <DataTemplate  DataType="{x:Type m:ContentA}">
        <Grid Background="Green">
            <TextBlock
                HorizontalAlignment="Center"
                VerticalAlignment="Center"
                FontSize="60"
                Text="ContentA" />
        </Grid>
    </DataTemplate>
    <DataTemplate  DataType="{x:Type m:ContentB}">
        <Grid Background="Red">
            <TextBlock
                HorizontalAlignment="Center"
                VerticalAlignment="Center"
                FontSize="60"
                Text="ContentB" />
        </Grid>
    </DataTemplate>
</ResourceDictionary>